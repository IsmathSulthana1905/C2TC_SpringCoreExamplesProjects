<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Bean Definition -->
    <bean id="student1" class="com.tnsif.SpringCoreExample.Student">
        <property name="Sid" value="101" />
        <property name="Sname" value="ismath" />
        <property name="city" value="villupuram"></property>
    </bean>
    <bean id="student2" class="com.tnsif.SpringCoreExample.Student">
        <property name="Sid" value="102" />
        <property name="Sname" value="harini" />
        <property name="city" value="pondy"></property>
    </bean>
    <bean id="student3" class="com.tnsif.SpringCoreExample.Student">
        <property name="Sid" value="103" />
        <property name="Sname" value="madhu" />
        <property name="city" value="chennai"></property>
    </bean>



</beans>